{
	"presets": [
		[
			"@babel/preset-react",
			{
				"pragma": "h",
				"pragmaFrag": "Fragment"
			}
		],
		[
			"@babel/preset-env",
			{
				"targets": {
					"esmodules": true
				}
			}
		],
		[
			"@emotion/babel-preset-css-prop",
			{
				"autoLabel": true,
				"labelFormat": "[local]"
			}
		]
	],
	"plugins": [
		"emotion",
		[
			"@babel/plugin-proposal-class-properties",
			{
				"loose": true
			}
		]
	]
}
